.images-container
  margin auto
  display grid
  grid-template-columns 20% 20% 20% 20%
  column-gap 20px
  row-gap 20px

  img
    width 100%
    box-shadow 1px 1px 3px rgba(0,0,0,0.5)

@media only screen and (min-width: 60em)
  .icons-container
    margin auto
    display grid
    grid-template-columns 20% 20% 20% 20%
    column-gap 6.6%
