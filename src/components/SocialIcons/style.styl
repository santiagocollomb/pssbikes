.social-icons-container
  display flex

  .social-icon
    width 25px
    height 25px
    background #fff
    border-radius 50%
    margin-right 0.7rem
    transition all 400ms cubic-bezier(0.22, 0.68, 0, 1.71)

    &:hover
      transform scale(1.05)

    .social-icons-link
      display flex
      height 100%

    img
      width 60%
      margin auto
